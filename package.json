    "gh-pages": "^2.1.1",
    "gsap": "^3.5.1",
    "less": "^3.13.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.3.0",
    "node-sass": "^4.14.1",
    "nuxt": "^2.14.12",
    "nuxt-sass-resources-loader": "^2.0.5",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "pug-plain-loader": "^1.0.0",
